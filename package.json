{
  "name": "nest",
  "version": "1.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "start": "tsc -b tsconfig.json -w",
    "demo": "node -e \"require('./dist').run()\"",
    "package": "yarn install --production --ignore-optional && zip -r bundle.zip ."
  },
